{
  "name": "filearchitect-app",
  "private": true,
  "version": "0.11.10",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/cleanup-ts-emit.mjs && tsc && vite build && node scripts/strip-crossorigin.mjs",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:build": "tauri build",
    "tauri:build:windows": "tauri build --target x86_64-pc-windows-msvc",
    "test": "vitest run",
    "test:watch": "vitest",
    "version:sync": "node scripts/version-sync.mjs",
    "version:check": "node scripts/version-check.mjs",
    "release:tag": "bash scripts/tag-release.sh",
    "clean": "cd src/ && find . -name '*.js' -type f -delete",
    "clean:tauri": "cd src-tauri && cargo clean",
    "clean:tauri:debug": "cd src-tauri && if exist target\\debug rmdir /s /q target\\debug",
    "tauri:dev": "cd ../filearchitect/ && npm run build && cd ../filearchitect-app/ && pnpm prune && pnpm update && npm run tauri dev",
    "tauri:dev:msvc": "tauri-dev.bat"
  },
  "dependencies": {
